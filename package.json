{
  "name": "eslint-config-zero",
  "version": "0.2.0",
  "description": "eslint zero config",
  "license": "MIT",
  "repository": "akameco/eslint-config-zero",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint .",
    "test": "yarn lint"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "eslint",
    "zero"
  ],
  "dependencies": {
    "eslint-config-precure": "^3.1.0",
    "ptils": "^0.2.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "eslint": "^5.6.1",
    "flow-bin": "^0.83.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  },
  "peerDependencies": {
    "eslint": ">=4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn flow && lint-staged"
    }
  }
}
