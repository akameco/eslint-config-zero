{
  "name": "eslint-config-zero",
  "version": "0.3.0",
  "description": "eslint zero config",
  "license": "MIT",
  "repository": "akameco/eslint-config-zero",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint .",
    "test": "yarn lint"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "eslint",
    "zero"
  ],
  "dependencies": {
    "eslint-config-precure": "4.2.0",
    "ptils": "0.3.0"
  },
  "devDependencies": {
    "all-contributors-cli": "5.11.0",
    "eslint": "5.13.0",
    "flow-bin": "0.92.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4"
  },
  "peerDependencies": {
    "eslint": ">=4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn flow && lint-staged"
    }
  }
}
